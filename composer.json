{
  "name": "asciisd/cashier",
  "description": "",
  "keywords": ["Laravel"],
  "authors": [
    {
      "name": "Amr Ahmed",
      "email": "aemad@asciisd.com"
    }
  ],
  "homepage": "https://github.com/asciisd/cashier",
  "require": {
    "php": "^7.2",
    "laravel/framework": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^4.0",
    "phpstan/phpstan": "^0.12.9"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Asciisd\\Cashier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Asciisd\\Cashier\\Tests\\": "tests/"
    }
  },
  "extra": {
